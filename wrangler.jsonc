{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "twilio-whatsapp-voice-agent",
  "main": "src/index.ts", // Points to index.ts in src directory
  "compatibility_flags": ["nodejs_compat"],
  "compatibility_date": "2025-05-25", // Using supported date to avoid warnings
  "assets": {
    "directory": "./public",
    "binding": "ASSETS"
  },
  "r2_buckets": [
    {
      "binding": "MY_BUCKET",
      "bucket_name": "audio"
    }
  ]
